{
    "moduleFileExtensions": [
        "js",
        "json",
        "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".int.e2e-spec.ts$",
    "transform": {
        "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFiles": [
        "dotenv/config"
    ],
    "moduleNameWrapper":{
        "src/(.*)":"<rootDir>/src/$1"
    }
}